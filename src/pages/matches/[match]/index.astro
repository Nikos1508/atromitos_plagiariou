---
import Layout from "../../../layout/default.astro"
import { getEntry } from "astro:content";

export function getStaticPaths() {
  return [
    {params: {match: '1'}},
    {params: {match: '2'}},
    {params: {match: '3'}}
  ];
}

let { match } = Astro.params;

match = (await getEntry('match', match)).data
---

<Layout title={`Αγώνας: ${match}`}>

    {match.teams}

</Layout>